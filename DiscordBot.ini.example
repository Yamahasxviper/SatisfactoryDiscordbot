[Discord]
# Discord Bot Configuration Template
# Copy this file to "DiscordBot.ini" and replace the placeholder values with your actual Discord bot credentials
# Note: DiscordBot.ini is in .gitignore to prevent committing sensitive credentials

# Your Discord bot token - get this from https://discord.com/developers/applications
BotToken=YOUR_BOT_TOKEN_HERE

# The Discord channel ID where the bot will send messages
ChatChannelId=YOUR_CHANNEL_ID_HERE

# Additional Discord bot settings can be added below
